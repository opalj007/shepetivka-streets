{
  "name": "streets",
  "version": "2.0.0",
  "description": "Browse renamed streets",
  "source": "src/index.html",
  "targets": {
    "default": {
      "distDir": "./flask-app/static"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel --dist-dir flask-app/static src/index.html",
    "clean": "rm -rf flask-app/static",
    "create-dist": "mkdir flask-app/static/",
    "copy-assets": "cp -r assets/* flask-app/static/",
    "build": "npm run clean && npm run create-dist && npm run copy-assets && parcel build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Oleksandr Palianytsia",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "http-proxy-middleware": "^2.0.6",
    "parcel": "^2.8.3",
    "process": "^0.11.10"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
